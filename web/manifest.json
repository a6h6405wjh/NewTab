{
  "name": "Personal new tab page",
  "description": "Custom new tab page",
  "background": 
  {
    "scripts": [ "default.js", "settings.js" ]
  },
  "permissions": [
    "bookmarks",
    "topSites",
    "chrome://favicon/",
    "storage",
    "tabs",
    "file: ///*" 
  ],
  "version": "0.1",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "default.html"
  },
  "manifest_version": 2
}